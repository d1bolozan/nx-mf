{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2021", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-mf/commons": ["libs/commons/src/index.ts"],
      "@nx-mf/navigation": ["libs/navigation/src/index.ts"],
      "@nx-mf/ui": ["libs/ui/src/index.ts"],
      "cart/Module": ["apps/cart/src/remote-entry.ts"],
      "chat/Module": ["apps/chat/src/remote-entry.ts"],
      "checkout/Module": ["apps/checkout/src/remote-entry.ts"],
      "flow_editor/Module": ["apps/flow_editor/src/remote-entry.ts"],
      "mdt_commons/Module": ["apps/mdt_commons/src/remote-entry.ts"],
      "mdt_commons/fetch": ["./libs/commons/src/lib/fetch/index.ts"],
      "mdt_commons/debounce": ["./libs/commons/src/lib/debounce/index.ts"],
      "mdt_commons/notifications": ["./libs/commons/src/lib/desktopNotificationsService/index.ts"],
      "mdt_commons/enums": ["./libs/commons/src/lib/enums/index.ts"],
      "mdt_commons/fileManager": ["./libs/commons/src/lib/fileManager/index.ts"],
      "mdt_commons/logger": ["./libs/commons/src/lib/logger/index.ts"],
      "mdt_commons/security": ["./libs/commons/src/lib/security/index.ts"],
      "mdt_commons/storageManager": ["./libs/commons/src/lib/storageManager/index.ts"],
      "mdt_commons/toastService": ["./libs/commons/src/lib/toastService/index.ts"],
      "mdt_commons/types": ["./libs/commons/src/lib/types/index.ts"],
      "mdt_commons/userService": ["./libs/commons/src/lib/userService/index.ts"],
      "mdt_commons/utils": ["./libs/commons/src/lib/Utils.ts"],
      "products/Module": ["apps/products/src/remote-entry.ts"],
      "storage_management/Module": ["apps/storage_management/src/remote-entry.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
